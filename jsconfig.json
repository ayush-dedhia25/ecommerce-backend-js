{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"#controllers/*": ["./src/controllers/*.js"],
			"#error-handlers/*": ["./src/error-handlers/*.js"],
			"#errors/*": ["./src/errors/*.js"],
			"#lib/*": ["./src/lib/*.js"],
			"#middlewares/*": ["./src/middlewares/*.js"],
			"#models/*": ["./src/models/*.js"],
			"#routes/*": ["./src/routes/*.js"],
			"#schemas/*": ["src/schemas/*.js"],
			"#utils/*": ["./src/utils/*.js"]
		}
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
